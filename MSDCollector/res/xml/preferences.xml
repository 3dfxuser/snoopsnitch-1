<?xml version="1.0" encoding="utf-8"?>

<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" > 
    
   	<PreferenceCategory android:title="@string/settings_cat_app_id"></PreferenceCategory><Preference android:key="settings_appId" android:title="@string/settings_applicationid_title" android:summary="@string/settings_applicationid_summary" /><PreferenceCategory android:title="@string/settings_cat_cleanup"></PreferenceCategory><ListPreference
        android:key="settings_basebandLogKeepDuration"
        android:title="@string/settings_basebandlogkeepduration_title"
        android:summary="%s"
        android:dialogTitle="@string/settings_basebandlogkeepduration_dialogtitle"
        android:entries="@array/settings_durations_entries"
        android:entryValues="@array/settings_durations_values"
        android:defaultValue="30" />
   	
  	<ListPreference
        android:key="settings_locationLogKeepDuration"
        android:title="@string/settings_locationlogkeepduration_title"
        android:summary="%s"
        android:dialogTitle="@string/settings_locationlogkeepduration_dialogtitle"
        android:entries="@array/settings_durations_entries"
        android:entryValues="@array/settings_durations_values"
        android:defaultValue="30" />
  	
  	<ListPreference
        android:key="settings_sessionInfoKeepDuration"
        android:title="@string/settings_sessioninfokeepduration_title"
        android:summary="%s"
        android:dialogTitle="@string/settings_sessioninfokeepduration_dialogtitle"
        android:entries="@array/settings_durations_entries"
        android:entryValues="@array/settings_durations_values"
        android:defaultValue="30" />
  	
	<ListPreference
        android:key="settings_cellInfoKeepDuration"
        android:title="@string/settings_cellinfokeepduration_title"
        android:summary="%s"
        android:dialogTitle="@string/settings_cellinfokeepduration_dialogtitle"
        android:entries="@array/settings_durations_entries"
        android:entryValues="@array/settings_durations_values"
        android:defaultValue="30" />
    
	
	
	
	
	<PreferenceCategory android:title="@string/settings_cat_log_encryption"></PreferenceCategory>
	
	<CheckBoxPreference 
	    android:key="settings_recordEncryptedDumpfiles"
        android:title="@string/settings_encrypteddumpfilesrecording_title"
        android:summary="@string/settings_encrypteddumpfilesrecording_summary"
        android:defaultValue="true"
 	/><CheckBoxPreference android:key="settings_recordUnencryptedDumpfiles" android:title="@string/settings_unencrypteddumpfilesrecording_title" android:summary="@string/settings_unencrypteddumpfilesrecording_summary" android:defaultValue="false" /><PreferenceCategory android:title="@string/settings_cat_location">
        </PreferenceCategory><CheckBoxPreference android:key="settings_gpsRecording" android:title="@string/settings_gpsrecording_title" android:summary="@string/settings_gpsrecording_summary" android:defaultValue="false" /><CheckBoxPreference android:key="settings_networkLocationRecording" android:title="@string/settings_networklocationrecording_title" android:summary="@string/settings_networklocationrecording_summary" android:defaultValue="true" /><PreferenceCategory android:title="@string/settings_cat_active_test"></PreferenceCategory>
	
	
	<CheckBoxPreference 
	    android:key="settings_active_test_force_offline"
        android:title="@string/settings_active_test_force_offline_title"
        android:summary="@string/settings_active_test_force_offline_summary"
        android:defaultValue="false"
 	/>
	<ListPreference
        android:key="settings_active_test_num_iterations"
        android:title="@string/settings_active_test_num_iterations_title"
        android:summary="%s"
        android:dialogTitle="@string/settings_active_test_num_iterations_dialogtitle"
        android:entries="@array/settings_active_test_num_iterations_entries"
        android:entryValues="@array/settings_active_test_num_iterations_values"
        android:defaultValue="5" />
	<CheckBoxPreference 
	    android:key="settings_active_test_disable_upload"
        android:title="@string/settings_active_test_disable_upload_title"
        android:summary="@string/settings_active_test_disable_upload_summary"
        android:defaultValue="false"
 	/>
	
	
	
	
	
    
</PreferenceScreen>